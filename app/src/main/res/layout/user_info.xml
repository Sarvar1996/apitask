<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <TextView
            android:text="TextView"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/project" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:textStyle="bold" android:textColor="@color/colorBlack"
            android:textSize="18sp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/star_big_on"
            android:id="@+id/imageView2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView5" android:layout_marginEnd="24dp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_menu_call"
            android:id="@+id/imageView5"
            app:layout_constraintEnd_toStartOf="@+id/imageView6" android:layout_marginEnd="24dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_input_add"
            android:id="@+id/imageView6" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="24dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="4444"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_like" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="@+id/imageView2"/>
    <TextView
            android:text="2222"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_folk"
            android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" app:layout_constraintEnd_toEndOf="@+id/imageView5"/>
    <TextView
            android:text="2222"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_watches" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" app:layout_constraintEnd_toEndOf="@+id/imageView6"
            android:layout_marginEnd="8dp"/>
</android.support.constraint.ConstraintLayout>